{
  "coc.preferences.formatOnSave": true,
  "[go][python][ts][typescript][css][sass][json]": {
    "coc.preferences.formatOnSave": true
  },
  "diagnostic.checkCurrentLine": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "prettier.requireConfig": true,
  "snippets.ultisnips.pythonPrompt": false
}
